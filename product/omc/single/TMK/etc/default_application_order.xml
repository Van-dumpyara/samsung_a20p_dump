            packageName="com.google.android.apps.subscriptions.red" 
            className="com.google.android.apps.subscriptions.red.LauncherActivity" />
    </folder>
    <!-- folder : Metro by T-Mobile -->
    <folder 
        screen="0" 
        title="Metro by T-Mobile"
        postPosition="true">  
         <!-- MetroZone -->
        <favorite 
            screen="0" 
            packageName="com.metropcs.metrozone" 
            className="com.mobileposse.firstapp.MainActivity" />
        <!-- MyMetro -->
        <favorite 
            screen="1" 
            packageName="com.nuance.nmc.sihome.metropcs" 
