        <favorite 
            screen="4" 
            packageName="com.samsung.android.oneconnect" 
            className="com.samsung.android.oneconnect.ui.SCMainActivity" />
        <!-- Secure Folder -->
        <favorite
            screen="5" 
            packageName="com.samsung.knox.securefolder" 
            className="com.samsung.knox.securefolder.switcher.SecureFolderShortcutActivity" />
        <!-- Smart Switch -->
        <favorite 
            screen="6"
            packageName="com.sec.android.easyMover" 
            className="com.sec.android.easyMover.DistributionActivity" />
