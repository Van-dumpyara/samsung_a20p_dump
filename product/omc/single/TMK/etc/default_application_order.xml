        <favorite 
            screen="4" 
            packageName="com.privacystar.android.metro" 
            className="com.tmobile.services.nameid.Startup.IdentifyingNumbersActivity" />	
		<!-- Network unlock -->
        <favorite 
            screen="5" 
            packageName="com.samsung.ssu" 
            className="com.samsung.ssu.IconMainActivity" />
    </folder>
    <!-- folder : Samsung -->
    <folder 
        screen="0" 
        postPosition="true"
        title="Samsung">
        <!-- My Files -->
        <favorite 
            screen="0" 
